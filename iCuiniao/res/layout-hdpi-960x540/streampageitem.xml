<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent">

	<FrameLayout android:layout_width="160px"
		android:layout_height="160px">
		<ImageView android:id="@+id/streampageitem"
			android:layout_width="160px" android:layout_height="160px"
			android:background="#00c9c9c9">
		</ImageView>
		<ImageView android:id="@+id/streampageitem_mark"
			android:layout_width="160px" android:layout_height="160px"
			android:background="#99000000" android:visibility="invisible">
		</ImageView>
	</FrameLayout>

</LinearLayout>
